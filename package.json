{
  "name": "portfolio",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "compile:scss": "sass sass/main.scss css/style.css --watch",
    "prefix:css": "postcss --use autoprefixer -b 'last 10 versions' css/style.css -o css/style.css",
    "compress:css": "sass css/style.css css/style.css --style=compressed",
    "build": "npm-run-all prefix:css compress:css",
    "dev": "node dev-server/server.js",
    "dev:scss": "npm-run-all --parallel compile:scss dev"
  },
  "author": "",
  "license": "GPL-3.0",
  "devDependencies": {
    "autoprefixer": "^10.3.1",
    "express": "^4.18.2",
    "multer": "^1.4.5-lts.1",
    "npm-run-all": "^4.1.5",
    "postcss-cli": "^8.3.1"
  },
  "dependencies": {
    "dotenv": "^16.3.1",
    "openai": "^4.24.1",
    "sass": "^1.88.0"
  }
}
